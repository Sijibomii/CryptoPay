{
  "name": "@sijibomi/cryptopay-sdk",
  "version": "3.1.0",
  "description": "sdk for github.com/sijibomii/CryptoPay",
  "main": "./lib/main.js",
  "scripts": {
    "build": "rimraf dist lib && npm run build:types && npm run build:js",
    "build:types": "tsc --declaration",
    "build:js": "babel src --out-dir lib --extensions \".tsx\" --ignore \"**/*.test.tsx\" --source-maps inline",
    "build:bundle": "webpack-cli --config ./webpack.config.js --mode production",
    "start": "webpack-dev-server --config ./example/webpack.config.js --mode development --open --hot"
  },
  "homepage": "https://github.com/Sijibomii/CryptoPay",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Ajayi sijibomi",
    "url": "https://github.com/Sijibomii/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sijibomii/CryptoPay"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@types/qrcode": "^1.5.1",
    "autoprefixer": "^10.4.14",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "typescript": "^5.1.6",
    "url-loader": "^4.1.1",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "qrcode": "^1.5.3",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  }
